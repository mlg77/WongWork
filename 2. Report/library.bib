file = {:C$\backslash$:/Users/Michelle/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Fabricius, Jensen, Lauritzen - 1993 - Microdialysis of interstitial amino acids during spreading depression and anoxic depolarization in.pdf:pdf},
isbn = {0006-8993 (Print)$\backslash$r0006-8993 (Linking)},
issn = {00068993},
journal = {Brain Research},
keywords = {Amino acid,Cerebral ischemia,Ischemic penumbra,Microdialysis,Migraine,Spreading depression,Stroke},
number = {1-2},
