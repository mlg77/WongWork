isbn = {1471-2105 (Electronic) 1471-2105 (Linking)},
issn = {14712105},
journal = {BMC Bioinformatics},
number = {SUPPL. 2},
pages = {1--13},
pmid = {18387205},
title = {{CellExcite: An efficient simulation environment for excitable cells}},
