isbn = {0031-9333 (Print)$\backslash$r0031-9333 (Linking)},
issn = {0031-9333},
journal = {Physiological reviews},
keywords = {brain disease model,dead-space microdomain,diffusion,extracellular,extracellular space,glia,macromolecule,matrix,tetramethylammonium,tortuosity,volume fraction,volume transmission},
number = {4},
pages = {1277--1340},
