journal = {Biophysical Journal},
number = {9},
pages = {2046--2054},
pmid = {24209849},
publisher = {Biophysical Society},
title = {{Potassium buffering in the neurovascular unit: Models and sensitivity analysis}},
url = {http://dx.doi.org/10.1016/j.bpj.2013.09.012},
volume = {105},
year = {2013}
}
