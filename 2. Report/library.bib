issn = {0022-3751},
journal = {Journal of Physiology},
keywords = {1982,211-230,331,physiol,pp,printed in great britain,with 9 text-ftigure8},
pages = {211--30},
pmid = {6296366},
title = {{Properties of single channel-activated potassium channels in cultured rat muscle}},
