file = {:C$\backslash$:/Users/Michelle/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Oberheim et al. - 2009 - Uniquely Hominid Features of Adult Human Astrocytes.pdf:pdf},
isbn = {0270-6474},
issn = {0270-6474},
journal = {Journal of Neuroscience},
number = {10},
pages = {3276--3287},
pmid = {19279265},
title = {{Uniquely Hominid Features of Adult Human Astrocytes}},
url = {http://www.jneurosci.org/cgi/doi/10.1523/JNEUROSCI.4707-08.2009},
volume = {29},
year = {2009}
