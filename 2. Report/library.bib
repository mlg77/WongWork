isbn = {0001-6322$\backslash$r1432-0533},
issn = {00016322},
journal = {Acta Neuropathologica},
keywords = {Astrocyte,Central nervous system,GFAP,Glial scar,Pathology,Reactive astrogliosis},
number = {1},
pages = {7--35},
pmid = {20012068},
primaryClass = {gr-qc},
